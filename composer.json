{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "barryvdh/laravel-debugbar": "3.4.2",
        "barryvdh/laravel-ide-helper": "^2.1",
        "box/spout": "~2.0",
        "doctrine/dbal": "^2.5",
        "endroid/qrcode": "1.2.4",
        "fideloper/proxy": "^4.0",
        "gigablah/sphinxphp": "^2.0",
        "google/apiclient": "1.0.*@beta",
        "imagine/imagine": "~0.5.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "php": "^7.1.3",
        "phpoffice/phpexcel": "1.*",
        "predis/predis": "^1.0",
        "rcrowe/twigbridge": "0.9.x",
        "twig/extensions": "^1.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "SzentirasHu\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ],
        "psr-4": {
            "SzentirasHu\\Test\\": "tests/feature",
            "SzentirasHu\\Test\\Smoke\\": "tests/smoke",
            "SzentirasHu\\Test\\Common\\": "tests/common"
        }

    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]    
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
