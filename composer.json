{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-ide-helper": "^3.5",
        "doctrine/dbal": "^3.0",
        "endroid/qrcode": "1.2.4",
        "gigablah/sphinxphp": "^2.0",
        "google/apiclient": "1.0.*@beta",
        "guzzlehttp/guzzle": "^7.0.1",
        "imagine/imagine": "~0.5.0",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.7",
        "laravel/tinker": "^2.5",
        "openspout/openspout": "^4.28",
        "phpoffice/phpspreadsheet": "^3.9",
        "predis/predis": "^1.0",
        "rcrowe/twigbridge": "0.14.3",
        "twig/extra-bundle": "^3.19",
        "twig/twig": "~3.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",        
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SzentirasHu\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ],
        "psr-4": {
            "SzentirasHu\\Test\\": "tests/feature",
            "SzentirasHu\\Test\\Smoke\\": "tests/smoke",
            "SzentirasHu\\Test\\Common\\": "tests/common"
        }

    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]    
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "prefer-stable": true

}
